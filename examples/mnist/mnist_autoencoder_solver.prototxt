net: "examples/mnist/mnist_autoencoder.prototxt"
test_iter: 500
test_interval: 1000
test_compute_loss: true
base_lr: 0.01
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 100
max_iter: 17990
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "examples/mnist/mnist_autoencoder"
momentum: 0.9
# solver mode: CPU or GPU
solver_mode: GPU
